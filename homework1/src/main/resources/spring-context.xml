<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

   <bean id="questionDao" class="ru.otus.vygovskaya.dao.QuestionDaoSimple">
      <constructor-arg name="questionsFileName" value="questions.csv"/>
    </bean>

    <bean id="questionService" class="ru.otus.vygovskaya.service.QuestionServiceImpl">
       <constructor-arg name="dao" ref="questionDao"/>
    </bean>

</beans>
